{
  "name": "urbanize",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run build:styles && npm run start_bulma",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:styles": "postcss src/styles.css -o src/styles.css",
    "css-build": "node-sass --omit-source-map-url src/bulma.scss src/css/bulma_styles.css",
    "css-watch": "npm run css-build -- --watch",
    "start_bulma": "npm run css-watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.2",
    "@angular/cdk": "^16.2.13",
    "@angular/common": "^17.1.2",
    "@angular/compiler": "^17.1.2",
    "@angular/core": "^17.1.2",
    "@angular/forms": "^17.1.2",
    "@angular/material": "^16.2.13",
    "@angular/platform-browser": "^17.1.2",
    "@angular/platform-browser-dynamic": "^17.1.2",
    "@angular/router": "^17.1.2",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "bulma": "^0.9.4",
    "crypto-js": "^4.2.0",
    "feather-icons": "^4.29.1",
    "flowbite": "^2.3.0",
    "ngx-pagination": "^6.0.3",
    "primeicons": "^6.0.1",
    "primeng": "^17.5.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.2",
    "@angular/cli": "^16.2.6",
    "@angular/compiler-cli": "^17.1.2",
    "@types/crypto-js": "^4.2.2",
    "@types/jasmine": "~4.3.0",
    "angular-crypto": "^0.0.3",
    "autoprefixer": "^10.4.17",
    "bulma": "^0.9.4",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
